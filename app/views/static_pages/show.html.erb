<div>
  <%= form_for(:session, url: login_path) do |f| %>
      <% if flash[:alert] %>
          <p id="notice"><%= flash[:alert] %></p>
      <% end %>

      <fieldset>
        <legend>Please Log In </legend>
        <div>
          <%= f.label :email%>
          <%= f.text_field :email%>
        </div>
        <div>
          <%= f.label :password%>
          <%= f.text_field :password%>
        </div>
        <div>
          <%= submit_tag "Login" %>
        </div>
      </fieldset>
  <% end %>

  <%= link_to 'Create Account', create_users_path %>
</div>